{
	"name": "template-controller",
	"version": "0.1.0",
	"description": "CasparCG HTML template controller.",
	"scripts": {
		"test": "echo 'Error: no test specified' && exit 1",
		"minify": "uglifyjs --ecma 5 --source-map filename=build/build.js.map -c -o build/build.min.js -- build/build.js",
		"minify-max": "uglifyjs --ecma 5 --source-map filename=build/build.js.map -c -m -o build/build.min.js -- build/build.js",
		"build": "tsc && npm run minify",
		"build-min": "tsc && npm run minify-max"
	},
	"repository": {
		"type": "git",
		"url": "git@kaidou-ren.srv.dolejska.me:casparcg/template-controller.git"
	},
	"keywords": [
		"casparcg",
		"html",
		"template",
		"animation"
	],
	"author": "Daniel Dolej≈°ka",
	"license": "proprietary",
	"devDependencies": {
		"typescript": "^3.6.4",
		"uglify-es": "^3.3.9"
	}
}
